generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  email     String   @unique
  firstName String
  lastName  String
  phone     String
  gender    Gender?
  countryCode String
  createdAt DateTime @default(now())
  createdBy String?
}

enum Gender {
  MALE
  FEMALE
}
